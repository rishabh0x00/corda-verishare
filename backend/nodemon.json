{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "dist"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "App restarted"
  },
  "watch": [
    "**/*.js",
    "**/*.ts"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js"
}
